<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:id="@+id/testBodyViewHolder_rootView"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<TextView
		android:id="@+id/testBodyViewHolder_titleView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>

	<TextView
		android:id="@+id/testBodyViewHolder_subtitleView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>

	<ImageView
		android:id="@+id/testBodyViewHolder_artworkView"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>

	<View
		android:id="@+id/testBodyViewHolder_contextualMenuButton"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"/>
</LinearLayout>